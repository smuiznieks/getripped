extends layout
block content
	.container
		.row
			.col.m4.s12
				h4 Humble Brag
				form
					.input-field
						input#uploadPhoto(placeholder="Upload photo" type="text")
					.input-field
						input#uploadCaption(placeholder="Upload caption" type="text")
					button#uploadPost(class="btn waves-effect waves-light" type="submit" name="action") Submit
			.col.m8.s12
				ul
					h4 Your feed
					each val in photos
						li.photo= val
					each val in captions
						li.caption= val
					each val in users
						li.user= val